{
  "name": "tsapi",
  "version": "0.0.1",
  "description": "Basic Typescript API",
  "main": "index.js",
  "scripts": {
    "backup": "ts-node tools/backup.ts",
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\"",
    "preprod": "ts-node tools/prepProductionConsole",
    "prod": "node ./dist/server.js",
    "bmf": "npm run build && npm run prod"
  },
  "author": "Duncan Pierce",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/validator": "^10.11.0",
    "db-migrate-mysql": "^1.1.10"
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/shelljs": "^0.8.5",
    "db-migrate": "^0.11.5",
    "fs-extra": "^8.0.1",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.5.1"
  }
}
